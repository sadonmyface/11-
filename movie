#학생1

import requests 
from bs4 import BeautifulSoup

cnt = 0
year = int(input())

url = f'https://movie.daum.net/ranking/boxoffice/yearly?date={year}'

response = requests.get(url)

if response.status_code == 200 :
  html = response.text 
  soup = BeautifulSoup(html,'html.parser')
  test = soup.find_all('a',{'class':'link_txt'})
  for code in test:
    cnt+=1
    print(cnt, code.get_text())
    if cnt == 50 :
      break

else :
  print('연결되지않음')
  print(response.status_code)







#학생2
import requests 
from bs4 import BeautifulSoup

cnt = 0
year = int(input())
date = []
title = []

url = f'https://movie.daum.net/ranking/boxoffice/yearly?date={year}'

response = requests.get(url)

# 개봉날짜, 관객수
if response.status_code == 200 :
  html = response.text 
  soup = BeautifulSoup(html,'html.parser')
  test = soup.find_all('span',{'class':'info_txt'})
  for code in test :
    cnt+=1
    date.append(code.get_text())
    if cnt == 100 :
      break
else :
  print('연결되지않음')
  print(response.status_code)

# 제목
if response.status_code == 200 :
  html = response.text 
  soup = BeautifulSoup(html,'html.parser')
  test = soup.find_all('a',{'class':'link_txt'})
  for code in test:
    cnt+=1
    title.append(code.get_text())
    if cnt == 50 :
      break
else :
  print('연결되지않음')
  print(response.status_code)

# 제목 입력 -> 개봉날짜, 관객수 
user = input()
n = title.index(user)
print(date[2*n])
print(date[2*n+1])
